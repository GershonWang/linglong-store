{
  "name": "linglong_store",
  "version": "1.2.4",
  "main": "dist-electron/main.js",
  "description": "简易式玲珑商店客户端",
  "author": "Jokul <986432015@qq.com>",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite --mode development",
    "pro": "vite --mode production",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview",
    "package": "electron-packager . Spliter --platform=win32 --arch=ia32 --overwrite --icon=icon.ico --out=./out --asar --app-version=0.0.1",
    "packager": "electron-packager . Spliter --platform=linux --arch=loongarch64 --overwrite --icon=512x512.png --out=./out --asar --app-version=0.0.1 --electron-zip-dir=/home/Jokul/Documents/",
    "packager1": "electron-packager . Spliter --platform=linux --arch=mips64el --overwrite --icon=512x512.png --out=./out --asar --app-version=0.0.1 --electron-zip-dir=/home/Jokul/Documents/",
    "deb1": "electron-installer-debian --src out/Spliter-linux-loongarch64 --dest out/deb/ --arch loongarch64",
    "deb": "electron-installer-debian --config config.json"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.4.0",
    "electron-log": "^5.0.3",
    "electron-updater": "^6.1.7",
    "element-plus": "^2.3.12",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "systeminformation": "^5.22.7",
    "vue": "^3.2.47",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "electron": "^28.2.1",
    "electron-builder": "^24.13.3",
    "electron-installer-debian": "^3.2.0",
    "electron-packager": "^17.1.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^1.4.2"
  }
}
